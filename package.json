{
  "name": "@yaegassy/coc-pug",
  "version": "0.1.1",
  "description": "Pug Language Features for coc.nvim",
  "author": "yaegassy <yosstools@gmail.com>",
  "license": "MIT",
  "main": "lib/index.js",
  "keywords": [
    "coc.nvim",
    "pug",
    "typescript",
    "javascript",
    "vim",
    "neovim"
  ],
  "engines": {
    "coc": "^0.0.80"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yaegassy/coc-pug"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint src --ext ts",
    "clean": "rimraf lib",
    "watch": "node esbuild.js --watch",
    "build": "node esbuild.js",
    "prepare": "node esbuild.js"
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 120,
    "semi": true
  },
  "devDependencies": {
    "@types/node": "^18.16.19",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "coc.nvim": "^0.0.83-next.9",
    "esbuild": "^0.16.17",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "~2.8.8",
    "rimraf": "^3.0.2"
  },
  "activationEvents": [
    "onLanguage:jade"
  ],
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "coc-pug configuration",
      "properties": {
        "pug.enable": {
          "type": "boolean",
          "default": true,
          "description": "Enable coc-pug extension."
        },
        "pug.trace.server": {
          "scope": "window",
          "type": "string",
          "enum": [
            "off",
            "messages",
            "verbose"
          ],
          "default": "off",
          "description": "Traces the communication between coc.nvim and the language server."
        },
        "pug.dev.serverPath": {
          "type": "string",
          "default": "",
          "description": "(For develop and check) Custom path to pug server module, ~ and $HOME, etc. can also be used. If there is no setting, the built-in module will be used "
        }
      }
    },
    "commands": []
  },
  "dependencies": {
    "@volar/language-server": "~1.8.0",
    "@volar/pug-language-server": "1.1.2",
    "typescript": "5.0.4"
  }
}
